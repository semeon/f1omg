body {
	font-family: <%= @main_font %>;
	background: <%= @body_background %>;
	margin: 0px;
	text-align: center;
}

div#f1omg_body {
	border: 0px solid <%= @border_color %>;
	padding: 0px;
	margin: 0px;
	text-align: center;
}

div#f1omg_toprow {
	border: 0px solid <%= @border_color %>;
	padding-right: 30px;
	margin: 0px;
	text-align: right;
	font-size: 12px;
}

div#f1omg_header {
	border: 0px solid <%= @border_color %>;
	border-bottom: 1px solid <%= @border_color %>;
	border-top: 1px solid <%= @border_color %>;
	padding: 0px;
	margin: 0px;
	text-align: center;
	vertical-align: bottom;
    background-image: url("/images/f1omg_back.png");
    background-repeat: repeat;
    background-position: top left;

}

#f1omg_logo {
	border: 0px solid <%= @border_color %>;

}

div#f1omg_navbar {
	border: 0px solid <%= @border_color %>;
    background-image: url("/images/f1omg_logo.png");
    background-repeat: no-repeat;
    background-position: top left;
	padding-top: 60px;
	padding-bottom: 0px;
	width: <%= @content_width %>;
	margin-left: auto;
	margin-right: auto;
	text-align: right;
	vertical-align: bottom;

}

.f1omg_menuitem {
	border: 0px solid <%= @border_color %>;
	text-align: center;
	padding: 0px;
	margin-left: 5px;
	margin-right: 5px;
	display: inline;
	font-size: 12px;
	font-weight: bold;	
	color: #9999ff;
}

.f1omg_menuitem a {
	color: #ccccff;
}


div#f1omg_content {
	border: 0px solid <%= @border_color %>;
	padding: 0px;
	width: <%= @content_width %>;
	margin-left: auto;
	margin-right: auto;
	text-align: left;

}

div#f1omg_footer {
	border: 0px solid <%= @border_color %>;
	border-top: 1px solid <%= @border_color %>;
	border-bottom: 0px solid <%= @border_color %>;
	padding: 0px;
	margin: 0px;
	text-align: center;
	font-size: 12px;
