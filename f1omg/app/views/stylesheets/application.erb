body {
	font-family: <%= @main_font %>;
	background: <%= @body_background %>;
	margin: 0px;
	text-align: center;
}

div#f1omg_body {
	border: 0px solid <%= @border_color %>;
	padding: 0px;
	margin: 0px;
	text-align: center;
}

div#f1omg_toprow {
	border: 0px solid <%= @border_color %>;
	padding-right: 30px;
	margin: 0px;
	text-align: right;
	font-size: 12px;
}

div#f1omg_header {
	border: 0px solid <%= @border_color %>;
	border-bottom: 1px solid <%= @border_color %>;
	border-top: 1px solid <%= @border_color %>;
	padding: 0px;
	margin: 0px;
	text-align: center;
	vertical-align: bottom;
	background: <%= @panel_backround %>;

}

div#f1omg_navbar {
	border: 0px solid <%= @border_color %>;
	width: 800px;
	margin-left: auto;
	margin-right: auto;
	text-align: right;

}

.f1omg_menutable {
	border-collapse: collapse;
	border: 1px solid <%= @border_color %>;
	
}

.f1omg_menuitem {
	border: 0px solid <%= @border_color %>;
	text-align: center;
	padding: 0px;
	margin-left: 5px;
	margin-right: 5px;
	display: inline;
	font-size: 12px;
	font-weight: bold;	
}

div#f1omg_content {
	border: 0px solid <%= @border_color %>;
	padding: 0px;
	width: 800px;
	margin-left: auto;
	margin-right: auto;
	text-align: left;

}

div#f1omg_footer {
	border: 0px solid <%= @border_color %>;
	border-top: 1px solid <%= @border_color %>;
	border-bottom: 1px solid <%= @border_color %>;
	padding: 0px;
	margin: 0px;
	text-align: center;
	font-size: 12px;
	background: <%= @panel_backround %>;
}
